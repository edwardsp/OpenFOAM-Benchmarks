FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ruotaant
{
    surfaces
    (
        "ruotaant.stl"
    );

    includedAngle           150;
    geometricTestOnly       yes;
    writeObj                yes;
    verboseObj              yes;
}

